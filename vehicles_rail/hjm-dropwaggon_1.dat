#
# Drop waggon
#
# author: Hj. Malthaner <h_malthaner@users.sourceforge.net>
#
# created:  2012/01/07
# update:   2012/01/12
#

#
# Vehicles have object type vehicle
#
obj=vehicle

#
# The name of the vehicle. Used as index in the translations files.
# If no translation is present, the text will be shown directly.
# Convention says, don't use spaces here.
#
name=hjm-dropwaggon_1
copyright=HjM

#
# The type of way needed for this vehicle to drive on.
# Allowed values are:
# air, monorail_track, maglev_track, narrowgauge_track
# road, track, tram_track, water
#
waytype=track

#
# Type of freight. This must match a freight in the pak set.
#
freight=Passagiere

#
# Payload, how many units of freight this vehicle can carry.
#
payload=40

#
# Top speed of the vehicle in km/h.
#
speed=120

#
# Vehicle price in credit cent
#
cost=100000

#
# Weight of the vehicle in tons without freight.
#
weight=12

#
# Vehicle length
#
length=8

#
# Power in KW
#
power=0

#
# Running cost per km
#
RunningCost=8

#
# Availability of the vehicle in the game (from ... to)
#
intro_year=1
intro_month=1
retire_year=5460
retire_month=1

#
# Images of the vehicle if empty
#
EmptyImage[W]=hjm-dropcar_1.0.1
EmptyImage[S]=hjm-dropcar_1.0.0
EmptyImage[E]=hjm-dropcar_1.0.1
EmptyImage[N]=hjm-dropcar_1.0.0
EmptyImage[SE]=hjm-dropcar_1.1.0
EmptyImage[NW]=hjm-dropcar_1.1.0
EmptyImage[NE]=hjm-dropcar_1.0.2
EmptyImage[SW]=hjm-dropcar_1.0.2
